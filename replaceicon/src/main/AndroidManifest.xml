<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="false">

        <activity android:name=".ReplaceIconActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:exported="false">

        </activity>

        <activity android:name=".CalculatorActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:exported="false">

        </activity>

        <activity android:name=".Calculator2Activity"
            android:screenOrientation="portrait"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize|uiMode"
            android:exported="false">

        </activity>
        <activity-alias
            android:name="com.hkbyte.CalculatorActivityAlias"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_alternate1"
            android:label="@string/app_name_alternate1"
            android:targetActivity="com.fuan.chameleon.WelcomeActivity"
            tools:ignore="IntentFilterExportedReceiver">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity-alias>

        <activity-alias
            android:name="com.hkbyte.CalculatorActivity2Alias"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_alternate2"
            android:label="@string/app_name_alternate1"
            android:targetActivity="com.fuan.chameleon.WelcomeActivity"
            tools:ignore="IntentFilterExportedReceiver">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity-alias>

    </application>
</manifest>